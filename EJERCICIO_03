<html><body>
    <script src = "ramda.min.js"> </script>
    <script>
    
    function isEven ( num ) { return ( num %2 == 0) ; }
    function find ( arr , f ){
        let new_arr=[];
        for(let i=0; i<arr.length;i++){
            if(f(arr[i])==true){
                new_arr.push(arr[i]);
            }
        }
        return new_arr;
    }
    
    isEven (3) // false
    isEven (4) // true
    
    result = find ([1 , 3, 5, 4, 2] , isEven );
    document.write("Find result : ", result ); // [4 , 2]
    
    </script></body>
    </html>
